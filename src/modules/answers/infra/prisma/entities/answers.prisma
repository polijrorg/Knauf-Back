model Answers {
  id         String      @id @default(uuid())
  answer     String
  created_at DateTime    @default(now())
  user       Users       @relation(fields: [userId], references: [id])
  userId     String
  question   Questions   @relation(fields: [questionId], references: [id])
  questionId String
}

model Users {
  id         String      @id @default(uuid())
  answers    Answers[]
}

model Questions {
  id         String      @id @default(uuid())
  answers    Answers[]
}
